# OS-independent files to link
linkfiles=(
    [.]=".aliases .agignore .bashrc .functions .gitconfig .gitignore_global .tmux.conf .tmux.default.theme .tmux.dracula.theme .zshrc"
    [.emacs.d]="init.el"
)

# Files to link, depending on OS
osfiles=(
    [.]=".minttyrc .zplug--packages.zsh .zshenv"
    [.ssh]="config"
)